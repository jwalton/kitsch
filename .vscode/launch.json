  "version": "0.2.0",
  "configurations": [
    {
      "name": "Show prompt",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}",
      "args": ["prompt"],
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Powerline prompt",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}",
      "args": ["prompt", "--config", "${workspaceFolder}/sampleconfig/powerline.yaml"],
      "cwd": "${workspaceFolder}",
      "env": { "FORCE_COLOR": "3" }
    }
  ]
}
